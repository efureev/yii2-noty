{
  "name": "efureev/yii2-noty",
  "description": "A Yii2 wrapper widget for the Noty, a noty plugin with jQuery.",
  "version": "0.1.1",
  "keywords": [
    "yii2",
    "widget",
    "extension",
    "noty",
    "noty",
    "alert"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/efureev/yii2-noty/issues",
    "source": "https://github.com/efureev/yii2-noty"
  },
  "authors": [
    {
      "name": "Feugene",
      "email": "fureev@gmail.com",
      "homepage": "https://github.com/efureev/yii2-noty"
    }
  ],
  "require": {
    "bower-asset/noty": "~2",
    "bower-asset/animate.css": "~3",
    "bower-asset/components-font-awesome": "*",
    "yiisoft/yii2": "~2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "efureev\\noty\\": "src/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
